<configuration>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />


    <add key="Microsoft.ServiceBus.eventHubNamespace" value="bandontherun-ns" />
    <add key="Microsoft.ServiceBus.eventHubPath" value="msbands" />
    <add key="Microsoft.ServiceBus.SendKeyName" value="D1" />
    <add key="Microsoft.ServiceBus.SendKeyValue" value="YOURKEY" />
